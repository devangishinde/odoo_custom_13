<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">
		
		<record id="warehouse_new" model="stock.warehouse">
            <field name="name">My New Company Warehouse</field>
            <field name="code">snw</field>
            <field name="company_id" ref="stock_extended.res_company_new"/>
            <field name="partner_id" ref="stock_extended.res_partner_company_new"/>
            <field name="active" eval="True"/>
        </record>
		
		<record id="sequence_picking_type_return" model="ir.sequence">
            <field name="name">My New Company Sequence RTN</field>
            <field name="code">stock.picking.type</field>
            <field name="prefix">RTN/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" ref="stock_extended.res_company_new"/>
             <field name="active" eval="True"/>
        </record>
		
		<record id="stock_picking_type_return" model="stock.picking.type">
            <field name="name">Return</field>
            <field name="sequence_code">RTN</field>
            <field name="code">incoming</field>
            <field name="company_id" ref="stock_extended.res_company_new"/>
            <field name="warehouse_id" ref="stock_extended.warehouse_new"/>
            <field name="sequence_id" ref="stock_extended.sequence_picking_type_return"/>
            <field name="active" eval="True"/>
        </record>
		
	</data>
</odoo>